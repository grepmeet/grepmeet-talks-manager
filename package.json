{
  "name": "grepmeet-talks-manager",
  "packageManager": "yarn@3.2.0",
  "private": true,
  "version": "1.0.0",
  "description": "grepmeet.org help communities to auto-organize talks selected by there members.",
  "repository": "git@github.com:grepmeet/grepmeet-talks-manager.git",
  "contributors": [
    "Quentin Machard <quentin.machard@gmail.com>",
    "Anas Ameziane <anas.ameziane@gmail.com>"
  ],
  "scripts": {
    "dev": "yarn workspace front run dev",
    "start": "yarn workspace front run start",
    "build": "yarn workspaces foreach -ptv run build",
    "lint": "yarn workspaces foreach -vp run lint",
    "commit": "git-cz"
  },
  "workspaces": [
    "packages/*"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/git-cz"
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@netlify/plugin-nextjs": "^4.4.0"
  }
}
